# Device configuration file for ATSAM3X8E
ARCH_DIR := $(PICORTOS_DIR)/arch/arm/cm3
DEVICE_DIR := $(PICORTOS_DIR)/devices/atmel/atsam3x8e

DEFINE := -DATSAM3X8E

INCLUDE := -I$(PICORTOS_DIR) -I$(PICORTOS_DIR)/arch/include
